@using OtelEleven.Models
@model List<ApplicationUser>
@{
    ViewBag.Title = "Üyeler";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
    Array list = Model.ToArray(); 
    }
 
<table class="table">
    <tr>
        <th>
            Kullanıcı Adı
        </th>
        <th>
           Ad
        </th>
        <th>
            Soyad
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
               @item.Kullaniciadi
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Soyad)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Kullaniciadi })  
            </td>
        </tr>
    }

</table>
